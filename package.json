{
  "name": "boilerplate",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check-types": "tsc --build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --cache --max-warnings 0",
